{
    "manifest_version": 2,
 
    "name": "Google Maps to Yandex/Graphhopper Path Converter",
    "version": "1.0.0.4",

    "background": {
      "scripts": [
        "content_scripts/background.js"
      ],
      "persistent": true
    },

    "browser_action": {
      "default_icon": "img/icon.png",
      "default_title": "Google Maps to Yandex/Graphhopper Path Converter",
      "default_popup": "html/popup.html"
    },

    "permissions": [
      "tabs","https://www.google.com/*","https://www.google.ru/*"
    ]
}